{
    "compilerOptions": {
      "target": "ES2021",
      "module": "CommonJS",
      "lib": ["ES2021", "DOM"],
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "esModuleInterop": true,
      "strict": true,
      "skipLibCheck": true,
      "downlevelIteration": true,
      "types": [
        "node",
        "@types/node",
        "playwright"
      ]
    },
    "include": [
      "**/*.ts"
    ]
  }
  